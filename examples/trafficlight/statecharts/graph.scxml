<?xml version="1.0" encoding="UTF-8"?>
<scxml xmlns="http://www.w3.org/2005/07/scxml" initial="GREEN" version="1.0">
    <state id="GREEN">
        <transition event="time" target="YELLOW"/>
    </state>
    <state id="YELLOW">
        <transition event="time" target="RED"/>
    </state>
    <state id="RED">
        <transition event="time" target="GREEN"/>
    </state>
</scxml>
