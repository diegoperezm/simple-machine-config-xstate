           [ [ 'A', 'GREEN' ], 'YELLOW', 'display' ],
           [ 'B', 'B', [ 'display', 'get' ] ],
           [ 'B', 'D', 'display' ]
				 ]

inputs   [
           [ 'A', 'GREEN', 'time' ],
           [ 'B', 'click' ],
